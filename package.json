{
  "name": "augurrank-firebase",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --experimental-https --hostname 0.0.0.0",
    "emu": "JAVA_TOOL_OPTIONS=\"-Xms2g -Xmx4g\" firebase emulators:start --project demo-augurrank",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "deploy-fstore-test": "firebase deploy --only firestore --project augurrank-test",
    "deploy-fstore-prod": "firebase deploy --only firestore --project augurrank-prod",
    "play-emu": "npx tsx --env-file=.env-scripts.emulator.local src/scripts/play.ts",
    "play-test": "npx tsx --env-file=.env-scripts.test.local src/scripts/play.ts",
    "play-prod": "npx tsx --env-file=.env-scripts.prod.local src/scripts/play.ts",
    "sc-test": "npx tsx --env-file=.env-scripts.test.local src/scripts/sc.ts",
    "sc-prod": "npx tsx --env-file=.env-scripts.prod.local src/scripts/sc.ts",
    "admin-test": "npx tsx --env-file=.env-scripts.test.local src/scripts/admin.ts",
    "admin-prod": "npx tsx --env-file=.env-scripts.prod.local src/scripts/admin.ts"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "@stacks/encryption": "^7.0.5",
    "@stacks/network": "^7.0.2",
    "@stacks/transactions": "^7.0.5",
    "clsx": "^2.1.1",
    "firebase": "^11.4.0",
    "firebase-admin": "^13.2.0",
    "immer": "^10.1.1",
    "jsontokens": "^4.0.1",
    "motion": "^12.5.0",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reselect": "^5.1.1",
    "sats-connect": "^3.3.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@headlessui/react": "^2.2.0",
    "@headlessui/tailwindcss": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@redux-devtools/extension": "^3.3.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "overrides": {
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2"
  }
}
